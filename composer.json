{
    "name": "ocular-nz/soundcloud",
    "type": "typo3-cms-extension",
    "description": "Soundcloud set as Content Element instead of HTML Element",
    "author": [],
    "require": {
        "typo3/cms-core": "^7.6.0 || ^8.7.0 || ^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Yahatix\\Soundcloud\\": "Classes"
        }
    },
    "replace": {
       "soundcloud": "self.version",
       "typo3-ter/soundcloud": "self.version"
    }
}
